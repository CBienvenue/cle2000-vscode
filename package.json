{
  "name": "cle2000",
  "displayName": "CLE-2000 Language Support",
  "description": "Syntax highlighting for the CLE-2000 language",
  "version": "0.0.6",
  "publisher": "charlesbienvenue",
  "engines": {
    "vscode": "^1.70.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CBienvenue/cle2000-vscode"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "cle2000",
        "aliases": ["CLE-2000", "cle2000"],
        "extensions": [".x2m",".c2m"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cle2000",
        "scopeName": "source.cle2000",
        "path": "./syntaxes/cle2000.tmLanguage.json"
      }
    ]
  }
}